<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="270.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.views.login.LogInController">
    <TextField fx:id="usernameField" layoutX="43.0" layoutY="163.0" onKeyPressed="#keyPressed" prefHeight="25.0" prefWidth="192.0" />
    <Button fx:id="enterButton" layoutX="113.0" layoutY="306.0" mnemonicParsing="false" onAction="#onEnterButton" text="Enter" />
   <Label fx:id="usernameLabel" layoutX="44.0" layoutY="148.0" prefHeight="15.0" prefWidth="60.0" text="Username">
      <font>
         <Font name="Courier New" size="12.0" />
      </font>
   </Label>
   <Label fx:id="passwordLabel" layoutX="45.0" layoutY="205.0" text="Password">
      <font>
         <Font name="Courier New" size="12.0" />
      </font>
   </Label>
   <Button fx:id="signUpButton" contentDisplay="TEXT_ONLY" layoutX="20.0" layoutY="264.0" mnemonicParsing="false" onAction="#onSignUp" prefHeight="25.0" prefWidth="230.0" text="Not a member? Sign up here." textOverrun="CLIP" wrapText="true">
      <cursor>
         <Cursor fx:constant="HAND" />
      </cursor>
   </Button>
   <PasswordField fx:id="passwordField" layoutX="43.0" layoutY="220.0" prefHeight="25.0" prefWidth="192.0" />
   <Label fx:id="welcomeLogin" layoutX="73.0" layoutY="60.0" text="Welcome">
      <font>
         <Font size="30.0" />
      </font></Label>
   <Button layoutX="121.0" layoutY="114.0" mnemonicParsing="false" onAction="#openUpdate" text="test" />
</AnchorPane>
