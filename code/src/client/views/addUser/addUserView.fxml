<AnchorPane prefHeight="161.0" prefWidth="297.0" style="-fx-border-color: orange; -fx-border-radius: 5; -fx-background-color: black;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.views.addUser.AddUserController">
    <TextField fx:id="usernameField" alignment="CENTER" layoutX="26.0" layoutY="26.0" onKeyPressed="#addUser" prefHeight="34.0" prefWidth="246.0" promptText="Input username" style="-fx-background-color: transparent; -fx-border-color: orange; -fx-prompt-text-fill: orange; -fx-border-radius: 5;">
